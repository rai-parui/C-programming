#include <stdio.h>

int main()
{
    float num1,num2;
    printf("\nEnter 1st number:");
    scanf("%f",&num1);
    printf("\nEnter 2nd number:");
    scanf("%f",&num2);
     printf("\nBefore swapping:\n");
    printf("1st number:%.2f \t 2nd number:%.2f",num1,num2);
    num1=num1-num2;
    num2=num1+num2;
    num1=num2-num1;
     printf("\nAfter  swapping:\n");
    printf("1st number:%.2f \t 2nd number:%.2f",num1,num2);
    return 0;
}
