#include <stdio.h>

int main()
{
    int num;
    printf("\nEnter a number:");
    scanf("%d",&num);
    if(num%3==0)
    {
        if(num%5==0)
        {printf("\nDivisible by both 3 and 5.");}
        else
        printf("\nNot divisible by 5.");
    }
    else
    printf("\nNot divisible by 3.");
   

    return 0;
}
