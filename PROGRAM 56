#include <stdio.h>

int isPrime(int n)
{ int flag=0;
    for(int i=2;i<=n/2;i++)
    {
       if(n%i==0)
       flag=1;
    }
    return flag;
}




int main()
{
    int num1,num2;
    printf("\nEnter the first  and the last number of the range :");
    scanf("%d %d",&num1,&num2);
    printf("\nThe prime numbers in the given range is: \n");
    for(;num1<=num2;num1++)
    {
        if(isPrime(num1)==0)
        printf("%d  ",num1);
        
    }
    
    return 0;
    
}
