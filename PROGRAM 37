#include <stdio.h>

int main()
{
    int num,sum=0;
    int original;
    printf("\nEnter a 3-digit number: ");
    scanf("%d",&num);
    original=num;
    while(num!=0)
    {
        sum=sum+(num%10)*(num%10)*(num%10);
        num/=10;
    }
    if(original==sum)
    {
        printf("\nArmstrong number.");
    }
    else
    {
        printf("\nNOT an Armstrong number.");
    }
    return 0;
}
