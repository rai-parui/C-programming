#include<stdio.h>
int main()
{
      
      int temp,square,count=1,number;

      printf("enter any number : ");
      scanf("%d",&number);
       temp=number;
       square=number*number;
           while (number!=0)
     {
          count=count*10;
          number=number/10;
     }
     
      if(square%count==temp)  
           printf(" \nAutomorphic Number");
      
      else
           printf("\nNot an Automorphic Number");
      return 0;
}
