#include <stdio.h>

int main()
{
   int arr[]={2,4,6,8,11,13,15,17,19,20};
   int flag=0;
   int srch,left,right,mid;
   printf("\nEnter the search element: ");
   scanf("%d",&srch);
   left=0,right=9;
   while(left<=right)
   {
       mid=(left+right)/2;
   if(srch==arr[mid])
   {flag=1;
   break;}
  else if(srch<arr[mid])
   
       right=mid-1;
   
   else if(srch>arr[mid])
   
       left=mid+1;
   
   }
   if(flag==1)
   printf("\nSEARCH SUCCESSFUL");
   else
   printf("\nSEARCH UNSUCCESSFUL.");

    return 0;
}
